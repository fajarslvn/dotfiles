{
	"suggest.noselect": false,
	"diagnostic.errorSign": "âœ˜",
	"diagnostic.warningSign": "!",
	"diagnostic.infoSign": "?",
	"diagnostic.checkCurrentLine": true,
	"coc.preferences.formatOnSaveFiletypes": [
		"javascript",
		"html",
		"json",
		"css",
		"scss",
		"go",
		"py",
		"md"
	],
	"coc.preferences.hoverTarget": "float",
	"languageserver": {
		"go": {
			"command": "gopls",
			"rootPatterns": [
				"go.mod"
			],
			"trace.server": "verbose",
			"filetypes": [
				"go"
			]
		}
	},
	"go.goplsOptions": {
		"staticcheck": true
	},
	"snippets.ultisnips.directories": [
		"UltiSnips",
		"gosnippets/UltiSnips"
	],
	"suggest.noselect": false,
	"python.pythonPath": "/Library/Frameworks/Python.framework/Versions/3.10/bin/python3"
}
